    # Data cleaning workflow
    analytics-bucket./upstream/cases/\<yyyy\>/\<pt\>/ -> vertex-workbench.data-cleaning-notebook: read excel
    analytics-bucket./upstream/outcomes/\<yyyy\>/\<pt\>/ -> vertex-workbench.data-cleaning-notebook: read excel
    vertex-workbench.data-cleaning-notebook -> analytics-bucket./cases/\<year=yyyy\>/\<pt=XX\>/: write parquet
    vertex-workbench.data-cleaning-notebook -> analytics-bucket./outcomes/\<year=yyyy\>/\<pt=XX\>/: write parquet
    # Data integration workflow
    analytics-bucket./cases/\<year=yyyy\>/\<pt=XX\>/ -> vertex-workbench.data-integration-notebook: read parquet
    analytics-bucket./outcomes/\<year=yyyy\>/\<pt=XX\>/ -> vertex-workbench.data-integration-notebook: read parquet
    vertex-workbench.data-integration-notebook -> analytics-bucket./merged/\<year=yyyy\>/\<pt=XX\>/: write parquet
    # Data analysis workflow
    analytics-bucket./merged/\<year=yyyy\>/\<pt=XX\>/ -> vertex-workbench.data-analysis-notebook: read parquet
    vertex-workbench.data-analysis-notebook -> analytics-bucket./outputs/\<year=yyyy\>/.reports
    vertex-workbench.data-analysis-notebook -> analytics-bucket./outputs/\<year=yyyy\>/.aggregated tables
    vertex-workbench.data-analysis-notebook -> analytics-bucket./outputs/\<year=yyyy\>/.graphs/plots
  }
